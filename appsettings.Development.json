{
  "RabbitMQ": {
    "Host": "rabbitmq"
  },
  "MongoDB": {
    "ConnectionString": "mongodb://root:example@mongo:27017",
    "DatabaseName": "Data",
    "SubmissionsCollectionName": "Submissions",
    "FilesCollectionName": "UserFiles",
    "ReceptorsCollectionName": "ReceptorFiles",
    "DockingResultsCollectionName": "Results"
  },
  "Redis": {
    "Host": "redis"
  },
  "Storage": {
    "Files": "/files"
  },
  "Client": {
    "Host": "http://localhost:3000" 
  },
  "Limitations": {
    "MaxReceptorSize": 1000,
    "MaxReceptorAmount": 1000,
    "MaxExhaustiveness": 8
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
